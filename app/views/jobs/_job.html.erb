<div class="col">
  <div class="card">
    <div class="card-body job" id="<%= dom_id(job) %>">
      <h5 class="card-title"><%= link_to_unless_current job.job_title, job %></h5>
      <h6 class="card-subtitle mb-2 text-muted"><%= job.location %></h6>
      <p class="card-subtitle mb-2 text-muted"><%= job.category %></p>
    </div>
  </div>
</div>
